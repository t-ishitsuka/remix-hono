{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@remix-hono-package/tsconfig/remix-config.json",
  "display": "ADMIN",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    },
    "rootDirs": [".", "./.react-router/types"]
  },
  "include": [
    "**/*",
    "**/.server/**/*",
    "**/.client/**/*",
    ".react-router/types/**/*"
  ]
}
